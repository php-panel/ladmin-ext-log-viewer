{
    "name": "php-panel/ladmin-log-viewer",
    "description": "Log viewer for laravel",
    "type": "library",
    "keywords": [
        "laravel-admin",
        "log",
        "viewer"
    ],
    "homepage": "https://github.com/php-panel/ladmin-log-viewer",
    "license": "MIT",
    "authors": [
        {
            "name": "leeqvip",
            "email": "leeqvip@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravel/framework": ">=12.0",
        "php-panel/laravel-admin": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~11.0"
    },
    "autoload": {
        "psr-4": {
            "Ladmin\\LogViewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ladmin\\LogViewer\\LogViewerServiceProvider"
            ]
        }
    }
}